<h3>Created Events: </h3>
<% current_user.created_events.each do |event| %>
<ul>
	<li>
		<%= event.name %>
		<%= link_to 'Show', event_path(event) %>
	</li>
</ul>
<% end %>

<h3>Atended Events: </h3>
<% current_user.attended_events.each do |event| %>
<ul>
	<li>
		<%= event.name %>
		<%= link_to 'Show', event_path(event) %>
	</li>
</ul>
<% end %>